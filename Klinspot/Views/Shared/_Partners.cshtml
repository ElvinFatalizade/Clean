@model List<Partner>
@{
    ViewBag.Title = "_Partners";
}

<!-- End: Client Section
  ==================================================-->
<div class="client-section" id="client-section">
    <!-- Container -->
    <div class="container">
        <div class="row">
            <!-- client list  -->
            <div class="owl-carousel owl-theme col-md-12" id="client">
                @foreach (var partner in Model)
                {
                    <div class="item row">
                        <div class="col-md-12">
                            <div class="client-box">
                                <a href="@partner.Url" target="_blank" class="client-box"></a>
                                <img alt="client" src="~/Uploads/@partner.Photo">
                            </div>
                        </div>
                    </div>
                }
               
            </div>
            <!--/ client list  -->
        </div>
        <!--/ row - -->
    </div>
    <!--/ Container - -->
</div>


