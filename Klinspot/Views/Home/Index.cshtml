@model Homepage

@{
    ViewBag.Title = "Index";
}




<!-- Start: Slides  -->
<div class="slides_wrapper">
    <div class="slider_home">

        @foreach (var sl in Model.Slides)
        {

            <div class="single_slider slide_bg_1" style="background-image:url('/Uploads/@sl.Photo')">
                <div class="slider_item_tb">
                    <div class="slider_item_tbcell">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-7 col-xs-12">
                                    <h2>
                                        <span class="slide_st1">
                                            @sl.Title
                                        </span>
                                    </h2>
                                    <p>@sl.Desc</p>
                                    @if (!string.IsNullOrEmpty(sl.BtnText))
                                    {
                                        <div class="slider_btn">
                                            <a href="@sl.BtnUrl" class="more-link slider_btn_one">@sl.BtnText</a>
                                          
                                        </div>
                                    }

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        }

    </div>
</div>
<!-- Start: Promo Section
==================================================-->
<div class="promo-area">
    <div class="container">
        <div class="promo-container">
            <div class="row">
                @foreach (var pr in Model.Promos)
                {
                    <div class="col-md-4 col-sm-4 ">
                        <div class="promo-wrap">
                            <div class="promo-icon">
                                <img src="~/Uploads/@pr.Icon" alt="@pr.Name">
                            </div>
                            <div class="promo-content">
                                <h4>@pr.Name</h4>
                                <p>@pr.Desc</p>
                            </div>
                        </div>
                    </div>
                    
                }

            </div>
        </div>
    </div>
</div>

<!-- End: Promo Section
==================================================-->
